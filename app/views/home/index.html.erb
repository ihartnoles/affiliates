<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%= page_entries_info @appointments %>
<%= will_paginate @appointments %>


<table>
	<th>ID</th>
	<th>Rank</th>
	<th>School</th>
	<% @appointments.each do |a| %>
	  <tr>
	    <td><%= a.id%></td>
	    <td><%= a.rank%></td>
	    <td><%= a.school%></td>
	  </tr>
	<% end %>
</table>